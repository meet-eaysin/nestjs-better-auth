{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"declaration": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true
	}
}
